{
  "name": "ooxml-templater",
  "version": "0.1.0",
  "description": "A JavaScript library for dynamic placeholder substitution in Office Open XML documents (.docx, .pptx, .xlsx)",
  "main": "src/index.js",
  "browser": "dist/ooxml-templater.min.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/integration",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "lint": "eslint src/**/*.js test/**/*.js",
    "lint:fix": "eslint src/**/*.js test/**/*.js --fix",
    "format": "prettier --write \"src/**/*.js\" \"test/**/*.js\" \"*.js\"",
    "format:check": "prettier --check \"src/**/*.js\" \"test/**/*.js\" \"*.js\"",
    "quality": "npm run lint && npm run format:check && npm run test"
  },
  "keywords": [
    "office",
    "docx",
    "pptx",
    "xlsx",
    "template",
    "placeholder",
    "substitution",
    "xml",
    "javascript"
  ],
  "author": "Milan G. <realmilangeorgijevic@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tmey2016/ooxml-templater.git"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@eslint/js": "^9.36.0",
    "babel-jest": "^30.2.0",
    "babel-loader": "^10.0.0",
    "buffer": "^6.0.3",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5",
    "webpack": "^5.102.0",
    "webpack-cli": "^6.0.1"
  },
  "peerDependencies": {
    "@zip.js/zip.js": "^2.7.0"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "node-fetch": "^2.7.0"
  }
}
